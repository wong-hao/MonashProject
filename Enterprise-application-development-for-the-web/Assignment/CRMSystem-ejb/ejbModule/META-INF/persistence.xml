<?xml version="1.0" encoding="UTF-8"?>
<persistence version="2.1"
	xmlns="http://xmlns.jcp.org/xml/ns/persistence"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/persistence
http://xmlns.jcp.org/xml/ns/persistence/persistence_2_1.xsd">
	<persistence-unit name="CRMSystem-ejb">
		<provider>org.eclipse.persistence.jpa.PersistenceProvider</provider>
		<jta-data-source>jdbc/ChestnutChestnut6</jta-data-source>
		<class>fit5042.assignm.repository.entities.Customer</class>
		<class>fit5042.assignm.repository.entities.CustomerContact</class>
		<class>fit5042.assignm.repository.entities.Groups</class>
		<class>fit5042.assignm.repository.entities.Users</class>
		<class>fit5042.assignm.repository.entities.Admin</class>
		<class>fit5042.assignm.repository.entities.Organisation</class>
		<exclude-unlisted-classes>true</exclude-unlisted-classes>
		<properties>
			<property name="eclipselink.ddl-generation" value="create" />
		</properties>
	</persistence-unit>
</persistence>